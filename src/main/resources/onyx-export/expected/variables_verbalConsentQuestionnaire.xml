   <variable name="consent_witness_cat"
             valueType="text"
             entityType="Participant">
      <categories>
         <category name="OPEN_AN"/>
      </categories>
      <attributes>
         <attribute name="condition" valueType="text">Questionnaire[VerbalConsentQuestionnaire.consent_acute.Y]</attribute>
         <attribute name="instructions" valueType="text" locale="en">Enter the name of the independent witness who has signed the form to confirm the participant gave verbal consent.</attribute>
         <attribute name="label" valueType="text" locale="en">Name of Witness</attribute>
         <attribute name="page" valueType="text">2</attribute>
         <attribute name="questionName" valueType="text">consent_witness_cat</attribute>
         <attribute name="questionnaire" valueType="text">VerbalConsentQuestionnaire</attribute>
         <attribute name="required" valueType="text">true</attribute>
         <attribute name="section" valueType="text">MAIN</attribute>
         <attribute name="stage" valueType="text">VerbalConsentQuestionnaire</attribute>
      </attributes>
   </variable>
